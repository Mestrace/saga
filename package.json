{
  "name": "saga.js",
  "productName": "saga.js",
  "version": "1.0.0",
  "description": "Saga is a semi-automated workflow tool for digital archivists to process digital images for publish.",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "build:python": "(cd pysaga && python -m poetry run build-executable && cd ..) && cpx \"./pysaga/dist/python/**/*\" \"./dist/python\"",
    "package": "npm run build:python && electron-forge package",
    "make": "npm run build:python && electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "Yida Liu",
    "email": "yxl1372@case.edu"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.8.3",
    "@electron-forge/maker-dmg": "^7.8.3",
    "@electron-forge/maker-rpm": "^7.8.3",
    "@electron-forge/maker-squirrel": "^7.8.3",
    "@electron-forge/maker-zip": "^7.8.3",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.3",
    "@electron-forge/plugin-fuses": "^7.8.3",
    "@electron-forge/plugin-vite": "^7.8.3",
    "@electron/fuses": "^1.8.0",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "electron": "37.3.0",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "typescript": "~4.5.4",
    "vite": "^5.4.19"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "electron-squirrel-startup": "^1.0.1",
    "python-shell": "^5.0.0"
  }
}
